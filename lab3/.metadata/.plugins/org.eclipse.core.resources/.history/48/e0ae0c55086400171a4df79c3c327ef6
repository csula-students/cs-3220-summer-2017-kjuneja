package midterm;

import java.util.ArrayList;
import java.util.List;


public class Restaurant {

	public final int id;
	public final String name;
	public final String url;
	public final List<Restaurant> designRatings = new ArrayList<>();
	public final List<Restaurant> tasteRatings = new ArrayList<>();
	public Restaurant(int id, String name, String url) {
		
		this.id = id;
		this.name = name;
		this.url = url;
	}
	public int getId() {
		return id;
	}
	public String getName() {
		return name;
	}
	public String getUrl() {
		return url;
	}
	public List<Restaurant> getDesignRatings() {
		return designRatings;
	}
	public List<Restaurant> getTasteRatings() {
		return tasteRatings;
	}
	
	
}
